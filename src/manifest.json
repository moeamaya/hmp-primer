{
  "name": "Wavma Browser",
  "version": "0.1.2",
  "permissions": ["activeTab", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "browser_action": {
    "default_icon": "icon-34.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content.bundle.js"]
    }
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
